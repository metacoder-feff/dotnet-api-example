{
  "openapi": "3.1.1",
  "info": {
    "title": "Example.Api | v1",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost/"
    }
  ],
  "paths": {
    "/api/v1/public/login": {
      "post": {
        "tags": [
          "LoginApiModule"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/v1/public/weatherforecast": {
      "get": {
        "tags": [
          "ExampleApiModule"
        ],
        "operationId": "GetWeatherForecast",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          }
        },
        "security": [
          {
            "Bearer-JWT": [ ]
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "LoginRequest": {
        "required": [
          "username",
          "password"
        ],
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "WeatherForecast": {
        "required": [
          "timestamp",
          "date",
          "temperature_c",
          "summary"
        ],
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "date": {
            "type": "string",
            "format": "date"
          },
          "temperature_c": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "summary": {
            "type": [
              "null",
              "string"
            ]
          },
          "temperature_f": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          }
        }
      }
    },
    "securitySchemes": {
      "Bearer-JWT": {
        "type": "http",
        "description": "Please enter token from '/login' output.\nExample: 'eyJh...mA' (without quotes).",
        "scheme": "Bearer",
        "bearerFormat": "Json Web Token (JWT)"
      }
    }
  },
  "tags": [
    {
      "name": "LoginApiModule"
    },
    {
      "name": "ExampleApiModule"
    }
  ]
}