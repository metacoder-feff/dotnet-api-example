{
  "openapi": "3.1.1",
  "info": {
    "title": "Example.Api | v1",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost/"
    }
  ],
  "paths": {
    "/weatherforecast": {
      "get": {
        "tags": [
          "Example.Api"
        ],
        "operationId": "GetWeatherForecast",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Instant": {
        "type": "string",
        "description": "Represents an instant in time (UTC) without time zone information.",
        "format": "date-time",
        "example": "2026-01-17T11:56:28.3295017Z"
      },
      "LocalDate": {
        "type": "string",
        "description": "A date without a time component or time zone.",
        "format": "date",
        "example": "2026-01-17"
      },
      "WeatherForecast": {
        "required": [
          "timestamp",
          "date",
          "temperature_c",
          "summary"
        ],
        "type": "object",
        "properties": {
          "timestamp": {
            "$ref": "#/components/schemas/Instant"
          },
          "date": {
            "$ref": "#/components/schemas/LocalDate"
          },
          "temperature_c": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "summary": {
            "type": [
              "null",
              "string"
            ]
          },
          "temperature_f": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "Example.Api"
    }
  ]
}